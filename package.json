{
  "name": "bs-mongodb",
  "description": "Bucklescript bindings to node mongodb driver",
  "version": "0.1.2",
  "scripts": {
    "build": "bsb -make-world",
    "start": "bsb -make-world -w",
    "clean": "bsb -clean-world",
    "test:run": "respect",
    "test": "run-s dev:start build test:run",
    "test:watch": "nodemon node_modules/re-respect/bin/respect",
    "dev:start": "docker-compose up -d",
    "dev:run": "run-p test:watch start",
    "dev": "run-s dev:start dev:run"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PeteProgrammer/bs-mongodb.git"
  },
  "keywords": [
    "bucklescript",
    "mongodb",
    "database",
    "node",
    "nosql"
  ],
  "bugs": {
    "url": "https://github.com/PeteProgrammer/bs-mongodb/issues"
  },
  "homepage": "https://github.com/PeteProgrammer/bs-mongodb#readme",
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@stroiman/async": "^0.7.0",
    "@stroiman/respect": "0.7.2",
    "bs-platform": "^2.2.3",
    "nodemon": "^1.12.1",
    "npm-run-all": "^4.1.2"
  },
  "dependencies": {
    "mongodb": "^2.2.33"
  }
}
